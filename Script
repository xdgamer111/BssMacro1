-- Scripts:

local function UGAJKTI_fake_script() -- Close.LocalScript 
	local script = Instance.new('LocalScript', Close)

	local CloseButton = script.Parent
	local BSSMacroHack = script.Parent.Parent
	
	CloseButton.MouseButton1Click:Connect(function()
		BSSMacroHack:Destroy()
	end)
	
end
coroutine.wrap(UGAJKTI_fake_script)()
local function SDLR_fake_script() -- Minimize.LocalScript 
	local script = Instance.new('LocalScript', Minimize)

	local MinimizeButton = script.Parent
	local BSSMacroHackStarterUI = MinimizeButton.Parent.Parent
	local BSSMacroHackFrame = BSSMacroHackStarterUI.MainFrame
	local PopUp = BSSMacroHackStarterUI.Popup
	
	local UIP = game:GetService("UserInputService")
	
	MinimizeButton.MouseButton1Click:Connect(function()
		BSSMacroHackFrame.Visible = false
		PopUp.Visible = true wait(5) PopUp.Visible = false
	end)
	
	UIP.InputBegan:Connect(function(input)
		if input.KeyCode == Enum.KeyCode.LeftControl and BSSMacroHackFrame.Visible == false then
			BSSMacroHackFrame.Visible = true
		end
	end)
end
coroutine.wrap(SDLR_fake_script)()
local function TWYQ_fake_script() -- Farm_2.LocalScript 
	local script = Instance.new('LocalScript', Farm_2)

	local screenUi = script:FindFirstAncestorOfClass("ScreenGui")
	
	if not screenUi then
		repeat task.wait()
			screenUi = script:FindFirstAncestorOfClass("ScreenGui")
		until screenUi
	end
	
	local PosPicker = script.Parent.Select
	local PosFrame = screenUi:WaitForChild("FarmingPosFrame")
	
	PosPicker.MouseButton1Click:Connect(function()
		PosFrame.Visible = not PosFrame.Visible
	end)
	
end
coroutine.wrap(TWYQ_fake_script)()
local function NMQOOLI_fake_script() -- BSSMacroHack.Tabs 
	local script = Instance.new('LocalScript', BSSMacroHack)

	local TabsButtons = script.Parent.MainFrame.InnerFrame.TabsButtons
	local ScrollingFrame = script.Parent.MainFrame.InnerFrame.ScrollingFrame
	
	for _, button in ipairs(TabsButtons:GetChildren()) do
		if button:IsA("TextButton") then
			button.MouseButton1Click:Connect(function()
				if button.Name == "Main" then
					ScrollingFrame.CanvasPosition = Vector2.new(0, 0)
				elseif button.Name == "Farm" then
					ScrollingFrame.CanvasPosition = Vector2.new(0, 230)
				elseif button.Name == "Quests" then
					ScrollingFrame.CanvasPosition = Vector2.new(0, 460)
				elseif button.Name == "Mobs" then
					ScrollingFrame.CanvasPosition = Vector2.new(0, 690)
				elseif button.Name == "Gear" then
					ScrollingFrame.CanvasPosition = Vector2.new(0, 920)
				elseif button.Name == "Crafting" then
					ScrollingFrame.CanvasPosition = Vector2.new(0, 1140)
				elseif button.Name == "Settings" then
					ScrollingFrame.CanvasPosition = Vector2.new(0, 1370)
				elseif button.Name == "Extra" then
					ScrollingFrame.CanvasPosition = Vector2.new(0, 1600)
				end
			end)
		end
	end
	
end
coroutine.wrap(NMQOOLI_fake_script)()
local function BEQOIM_fake_script() -- BSSMacroHack.LocalScript 
	local script = Instance.new('LocalScript', BSSMacroHack)

	local UserInputService = game:GetService("UserInputService")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local Players = game:GetService("Players")
	local Workspace = game:GetService("Workspace")
	local RunService = game:GetService("RunService")
	
	local player = Players.LocalPlayer
	local playerGui = player:WaitForChild("PlayerGui")
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoid = character:WaitForChild("Humanoid")
	local spawnPosCFrame = player:WaitForChild("SpawnPos").Value
	local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
	local cap = player:WaitForChild("CoreStats"):WaitForChild("Capacity").value
	local ToolCollect = require(ReplicatedStorage.Events).ClientCall
	local PlayerHiveCommand = require(ReplicatedStorage.Events).ClientCall
	print(cap)
	local savedPositions = {}
	local farmingActive = false
	local moveDistance = 15
	local walkDuration = 1.5
	local turnAngle = 90
	local cycleCount = 10
	local fieldTime = 60
	local alwaysPresentTokens = {}
	local collectionRadius = 10 -- Distance within which tokens are collected
	
	-- Assuming you have a ScreenGui named 'BSSMacroHack' and a Frame named 'FarmingPosFrame'
	local screenGui = playerGui:WaitForChild("BSSMacroHack")
	local frame = screenGui:WaitForChild("FarmingPosFrame")
	
	-- Connect buttons to save positions
	for i = 1, 17 do
		local button = frame:WaitForChild(tostring(i)) -- Buttons named "1", "2", etc.
		button.MouseButton1Click:Connect(function()
			savedPositions[i] = player.Character.HumanoidRootPart.CFrame
			print("Position " .. i .. " saved: " .. tostring(savedPositions[i]))
		end)
	end
	
	--//clear saved positions
	local clearPositionBtn =screenGui:FindFirstChild("Clear", true)
	clearPositionBtn.MouseButton1Click:Connect(function()
		farmingActive = false
		task.wait(0.05)
		savedPositions = {}
		print("Cleared saved positions; stopped farming.")
	end)
	
	-- Connect TextBox to enter the cycle count
	local cycleBox = screenGui:FindFirstChild("LoopTime", true) -- Assuming your TextBox is named "CycleCountBox"
	cycleBox.Text = tostring(cycleCount)
	
	cycleBox.FocusLost:Connect(function(enterPressed)
		if enterPressed then
			local newCount = tonumber(cycleBox.Text)
			if newCount and newCount > 0 then
				cycleCount = newCount
				print("Cycle count updated to: " .. cycleCount)
			else
				cycleBox.Text = tostring(cycleCount)
				print("Invalid cycle count entered.")
			end
		end
	end)
	
	-- Connect TextBox to enter the field time
	local fieldTimeBox = screenGui:FindFirstChild("TimePerField", true) -- Assuming your TextBox is named "FieldTimeBox"
	fieldTimeBox.Text = tostring(fieldTime)
	
	fieldTimeBox.FocusLost:Connect(function(enterPressed)
		if enterPressed then
			local newTime = tonumber(fieldTimeBox.Text)
			if newTime and newTime > 0 then
				fieldTime = newTime
				print("Field time updated to: " .. fieldTime .. " seconds")
			else
				fieldTimeBox.Text = tostring(fieldTime)
				print("Invalid field time entered.")
			end
		end
	end)
	
	-- Function to move the player forward by a specified distance
	local function moveForward()
		local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
		local initialPosition = humanoidRootPart.Position
		local direction = humanoidRootPart.CFrame.LookVector
		humanoid:MoveTo(initialPosition + direction * moveDistance)
		wait(walkDuration)
	end
	
	-- Function to turn the player left by a specified angle
	local function turnLeft()
		local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
		local currentCFrame = humanoidRootPart.CFrame
		humanoidRootPart.CFrame = currentCFrame * CFrame.Angles(0, math.rad(-turnAngle), 0)
	end
	
	-- Function to make the player walk in a square pattern for a specified amount of time
	local function walkInSquare()
		local endTime = tick() + fieldTime
		while tick() < endTime do
			if not farmingActive then return end
			moveForward()
			turnLeft()
			wait(0.5)
			ToolCollect("ToolCollect")
		end
	end
	
	-- Function to move the player towards a specified token
	local function walkToToken(token)
		local targetPosition = token.Position
		local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
		local direction = (targetPosition - humanoidRootPart.Position).unit  -- Unit vector direction
	
		humanoid:MoveTo(targetPosition)  -- Direct the humanoid to move towards the token
	
		while (humanoidRootPart.Position - targetPosition).Magnitude > collectionRadius do
			wait(0.1)  -- Check periodically until the player reaches the token
		end
	end
	
	-- Function to collect a specified token
	local function collectToken(token)
		if token and token:IsA("BasePart") then
			local distance = (token.Position - humanoidRootPart.Position).Magnitude
			if distance <= collectionRadius then
				walkToToken(token)  -- Walk to the token
				print("Token collected: " .. token.Name)
				-- Perform collection logic here if necessary (e.g., increment inventory, remove token)
			end
		end
	end
	
	-- Function to teleport the player to a specified position
	local function teleportTo(cframe)
		local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
		humanoidRootPart.CFrame = cframe
	end
	
	-- Function to check if the player's pollen is zero
	local function isPollenZero()
		local coreStats = player:WaitForChild("CoreStats")
		local pollen = coreStats:FindFirstChild("Pollen")
		return pollen and pollen.Value <= 0
	end
	
	-- Function to check if the player's pollen is at max capacity
	local function isPollenMax()
		local pol = player:WaitForChild("CoreStats"):WaitForChild("pollen").value
		print(pol-cap)
		if pol-cap <= 0 then 
			teleportCycleCount = cycleCount
		end
	end
	
	-- Function to start the farming process
	local function startFarming()
		farmingActive = true
		local teleportCycleCount = 0
		while farmingActive do
			for _, cframe in pairs(savedPositions) do
				if not farmingActive then return end
				teleportTo(cframe)
				walkInSquare()
				wait(0.5)
				isPollenMax()
				teleportCycleCount = teleportCycleCount + 1
	
				if (teleportCycleCount >= cycleCount) then
					print(cycleCount .. " cycles completed. Teleporting to SpawnPos.")
					teleportTo(spawnPosCFrame)
					wait(1)
					PlayerHiveCommand("PlayerHiveCommand", "ToggleHoneyMaking")
	
					while not isPollenZero() do
						wait(1)
					end
	
					wait(10)
					PlayerHiveCommand("PlayerHiveCommand", "ToggleHoneyMaking")
					print("Conversion completed. Waiting 10 seconds.")
	
					teleportCycleCount = 0
				end
			end
			
		end
	end
	
	-- Function to stop the farming process
	local function stopFarming()
		farmingActive = false
		print("Farming stopped.")
	end
	
	-- Function to check if a token is new (not always present)
	local function isNewToken(token)
		return not alwaysPresentTokens[token]
	end
	
	-- Connect function to check for new tokens during each frame
	RunService.Heartbeat:Connect(function()
		if farmingActive then
			for _, token in pairs(Workspace.Collectibles:GetChildren()) do
				if isNewToken(token) then
					collectToken(token)
				end
			end
		end
	end)
	
	-- Connect function to handle new tokens as they are added
	Workspace.Collectibles.ChildAdded:Connect(function(token)
		if farmingActive and isNewToken(token) then
			collectToken(token)
		end
	end)
	
	-- Connect function to handle user input for starting and stopping farming
	UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)
		if not gameProcessedEvent then
			if input.KeyCode == Enum.KeyCode.F and input:IsModifierKeyDown(Enum.ModifierKey.Ctrl) then
				print("Detected Keys Pressed!")
				if not farmingActive then
					print("Starting farming!")
					startFarming()
				else
					print("Already farming!")
				end
			elseif input.KeyCode == Enum.KeyCode.X and input:IsModifierKeyDown(Enum.ModifierKey.Ctrl) then
				stopFarming()
			end
		end
	end)
	
end
coroutine.wrap(BEQOIM_fake_script)()
